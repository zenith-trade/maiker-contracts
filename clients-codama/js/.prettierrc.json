{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "arrowParens": "always",
  "printWidth": 120,
  "parser": "typescript",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "requirePragma": true,
  "insertPragma": true
}
